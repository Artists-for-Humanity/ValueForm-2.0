{
    "version": 2,
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "sanity-studio/dist/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/index.js" },
      { "src": "/admin/static/(.*)", "dest": "/sanity-studio/dist/static/$1" },
      { "src": "/admin/(.*)", "dest": "/sanity-studio/dist/index.html" },
      { "src": "/", "dest": "/index.html" }
    ]
  }
   