{
    "builds": [
      {
        "src": "sanity-studio/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "." }
      }
    ],
    "rewrites": [
      {
        "source": "/admin/(.*)",
        "destination": "/sanity-studio/dist/index.html"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  